{
  "manifest_version": 2,
  "name": "Development Workflow",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icon2.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}